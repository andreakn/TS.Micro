@model Timesheet.Micro.Models.Domain.Model.Customer


<input type="hidden" name="Id" value="@Model.Id"/>
<div class="form-group">
    @Html.LabelFor(x => x.IsActive, "Aktiv kunde")
    @Html.CheckBoxFor(x => x.IsActive)
</div>
<div class="form-group">
    @Html.LabelFor(x=>x.Name,"Firma")
    @Html.TextBoxFor(x => x.Name)
</div>
<div class="form-group">
    @Html.LabelFor(x => x.Description, "Beskrivelse")
    @Html.TextBoxFor(x => x.Description)
</div>
