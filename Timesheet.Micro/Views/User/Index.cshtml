@model List<Timesheet.Micro.Models.Domain.Model.User>
@{
    ViewBag.Title = "Index";
}

<h2>Brukere</h2>@Html.ActionLink("Opprett bruker", "Create")


<ul class="list-group">
    @foreach (var user in Model)
    {
        <li class="list-group-item">@Html.ActionLink(user.Username, "SetPassword", new { username = user.Username }, new { })</li>
    }
    @Html.ActionLink("Opprett bruker", "Create")
</ul>
